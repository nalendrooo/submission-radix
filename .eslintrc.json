{
    "env": {
      "browser": true,
      "es2021": true,
      "cypress/globals": true
    },
    "plugins": [
      "cypress"
    ],
    "extends": "airbnb",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-alert": "off",
      "react/no-danger": "off",
      "jsx-a11y/click-events-have-key-events": "off",
      "jsx-a11y/no-static-element-interactions": "off",
      "react/forbid-prop-types": "off",
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
      }
  }